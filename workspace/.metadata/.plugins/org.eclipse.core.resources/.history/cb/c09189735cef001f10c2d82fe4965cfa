<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
	Cookie[] arr = request.getCookies();
	
	String userId = "";
	if(arr != null) {
		for(Cookie c : arr) {
			
			if(c.getName().equals("userId") ) {
				userId = c.getValue();
			}
			
		}
	}
	
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	
	<%=userId %>님 환영합니다!
	
</body>
</html>